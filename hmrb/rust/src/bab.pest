name = { (ASCII_ALPHANUMERIC | "-" | "_")+ }
value = { (ASCII_ALPHANUMERIC | "-" | "_")+ }
attribute = {"(" ~ name ~ ":" ~ value ~ ")"}
attributes = { "(" ~ attribute* ~ ")" }
var = {"Var" ~ name ~ ":" ~ attributes}
law = {"Law" ~ name? ~ ":" ~ attributes}
grammar = { (law | var)* }

WHITESPACE = _{ ("\n" | " ") }
